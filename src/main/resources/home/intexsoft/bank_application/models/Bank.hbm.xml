<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping">

    <class name="home.intexsoft.bank_application.models.Bank" table="banks">
        <id name="id" column="id"/>
        <property name="name" column="bank_name"/>
        <property name="commissionForIndividual" column="commission_for_individual"/>
        <property name="commissionForEntity" column="commission_for_entity"/>

    </class>
</hibernate-mapping>